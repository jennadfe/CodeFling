        function removeOpenMenu(theThis) {
            //if (theThis.parent().parent().attr('id') != 'regionMapEnglandRegions') {
            if (theThis.attr('id').indexOf('sidemenu') != -1) {

                hmrs = [
                    ".highlightMapRegionEastMidlands",
                    ".highlightMapRegionEastofEngland",
                    ".highlightMapRegionLondon",
                    ".highlightMapRegionNorthEast",
                    ".highlightMapRegionNorthWest",
                    ".highlightMapRegionSouthEast",
                    ".highlightMapRegionSouthWest",
                    ".highlightMapRegionWestMidlands",
                    ".highlightMapRegionYorkshireandtheHumber"
                ];

                for (i = 0; i < hmrs.length; i++) {
                    $('' + hmrs[i]).find('a').removeClass("open");
                    $('' + hmrs[i]).find('a').addClass("closed");
                    $('' + hmrs[i]).find('ul').css({ height: "0px" });
                    $('' + hmrs[i]).find('ul').find('li').css({ display: "none" });
                }
            }
        }

        function addOpenMenu(theThis) {
            theThis.find('a').first().removeClass("closed");
            theThis.find('a').first().addClass("open");
            theThis.find('ul').first().css({ height: "auto" });
            theThis.find('ul').find('li').css({ display: "list-item" });
        }

        $('.highlightMapRegionEastMidlands').hover(function () {
            removeOpenMenu($(this));
            addOpenMenu($(this));
